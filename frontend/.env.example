# Backend API URL
REACT_APP_BACKEND_URL=http://localhost:8001/api

# WebSocket will automatically use the same host as REACT_APP_BACKEND_URL

# Optional: Enable visual edits (development only)
REACT_APP_ENABLE_VISUAL_EDITS=false

# Optional: Health check (development only)
ENABLE_HEALTH_CHECK=false

# Development server configuration
HOST=0.0.0.0
PORT=3000

# WebSocket configuration (for production deployments)
WDS_SOCKET_PORT=443
