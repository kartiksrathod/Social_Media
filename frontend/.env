# Backend API URL (this is correct for your local machine)
REACT_APP_BACKEND_URL=http://localhost:8001/api

# Optional: Enable visual edits (keep false)
REACT_APP_ENABLE_VISUAL_EDITS=false

# Optional: Health check flag (keep false)
ENABLE_HEALTH_CHECK=false

# Development server config (leave as is)
HOST=0.0.0.0
PORT=3000

# WebSocket port for production. For local dev, ignore it.
# WDS_SOCKET_PORT=443

# Cloudinary (fill these two)
REACT_APP_CLOUDINARY_CLOUD_NAME=dmcerpxjs
REACT_APP_CLOUDINARY_UPLOAD_PRESET=socialvibe_unsigned
